
<header id="applicants-header">
    <div id="applicants-container">
        <div id="applicants-heading">
            <ul>
                <li>#</li>
                <li>Name</li>
                <li>Email</li>
                <li>Contact</li>
                <li>Resume</li>
                <% if(app) { %>
                    <li>Applied For</li>
                    <li>Update Btn</li>
                <% } %>
            </ul>
        </div>
        <div id="applicants">
            <!--------------->
            <% applicants.forEach(applicant=>{ %>
                <div class="applicants-details">
                    <ul>
                        <li><%= count++ %></li>
                        <li><%= applicant.name %></li>
                        <li><%= applicant.email %></li>
                        <li><%= applicant.number %></li>
                        <li><a href="<%= applicant.resume %>" target="_blank">View</a></li>
                        <% if(app) { %>
                            <li><%= applicant.companyName %></li>
                            <li>
                                <a id="app-edit" href="#"><i class="fa-regular fa-pen-to-square"></i></a>
                                <a id="app-delete" href="#"><i class="fa-regular fa-trash-can"></i></a>
                            </li>
                        <% } %>
                    </ul>
                </div>
            <% }) %>
        </div>
    </div>
</header>